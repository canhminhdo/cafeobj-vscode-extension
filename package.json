{
    "name": "cafeobj",
    "displayName": "CafeOBJ",
    "description": "Language Cafeobj Support",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Themes"
    ],
    "contributes": {
        "iconThemes": [
            {
                "id": "icon-cafeobj",
                "label": "Cafeobj Icon",
                "path": "./icons/vs-cafeobj-icon-theme.json"
            }
        ],
        "languages": [
            {
                "id": "cafeobj",
                "aliases": [
                    "CafeOBJ",
                    "cafeobj"
                ],
                "extensions": [
                    ".cafe"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cafeobj",
                "scopeName": "source.cafeobj",
                "path": "./syntaxes/cafeobj.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "cafeobj",
                "path": "./snippets/cafeobj.snippets.json"
            }
        ]
    }
}